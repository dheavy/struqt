{
  "name": "struqt",
  "version": "0.3.3",
  "description": "Implementation of several common data structures.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./*.js ./test",
    "prepublish": "npm test && npm run clean",
    "test": "npm run lint && mocha -u bdd",
    "test:cover": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --dir ./test",
    "clean": "rimraf -r ./docs",
    "doc": "jsdoc -c ./jsdoc.json "
  },
  "engines": {
    "node": ">=5"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dheavy/struqt.git"
  },
  "keywords": [
    "data structures",
    "datastructures"
  ],
  "author": "Davy Peter Braun <hello@davybraun.com> (http://davybraun.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dheavy/struqt/issues"
  },
  "homepage": "https://github.com/dheavy/struqt#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.2.2",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-standard": "latest",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.2",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "rimraf": "^2.5.4"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "semi": 0
    },
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "dependencies": {
    "lodash.isequal": "^4.4.0"
  }
}
