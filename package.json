{
  "name": "struqt",
  "version": "1.0.0",
  "description": "JavaScript implementation of several common data structures.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./*.js ./test",
    "prepublish": "npm test && npm run clean && npm run build",
    "test": "npm run lint && mocha -u bdd --use_strict --compilers js:babel-register",
    "test:cover": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "test:coveralls": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "clean": "rimraf -r ./docs",
    "doc": "jsdoc -c ./jsdoc.json ",
    "build": "WEBPACK_ENV=build webpack",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch"
  },
  "engines": {
    "node": ">=5"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dheavy/struqt.git"
  },
  "keywords": [
    "data structures",
    "datastructures",
    "linked list",
    "singly linked list",
    "doubly linked list",
    "tree",
    "graph",
    "sorting"
  ],
  "author": "Davy Peter Braun <hello@davybraun.com> (http://davybraun.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dheavy/struqt/issues"
  },
  "homepage": "https://github.com/dheavy/struqt#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-standard": "latest",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.2",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.3",
    "webpack-node-externals": "^1.5.4"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "semi": 0,
      "eol-last": 0
    },
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "dependencies": {
    "deep-equal": "^1.0.1"
  }
}
